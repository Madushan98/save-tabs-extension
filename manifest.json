{
    "manifest_version": 3,
    "name": "Save Tabs State",
    "version": "1.0",
    "description": "Save the current open tabs as a state.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }